 || correo === undefined || direccion === undefined || ciudad === undefined || telefono === undefined) {\r\n            res.status(400).json({ message: \"por favor ingrese los campos correspondientes.\" });\r\n        }\r\n    \r\n        const datos = { nombre, correo, direccion , ciudad, telefono };\r\n        console.log(datos)\r\n        const connection = await getConnection();\r\n        const result = await connection.query(\"UPDATE cliente SET ? WHERE id_cliente = ?\", [datos,id_cliente]);\r\n        res.json(result,  'hola');\r\n\r\n    } catch (error) {\r\n        res.status(500);\r\n        res.send(error.message);\r\n    }\r\n};\r\n\r\n/*function verificarToken(req, res, next) {\r\n    const bearerHeader = req.headers['authorization'];\r\n\r\n    if (typeof bearerHeader !== 'undefined') {\r\n        const bearerToken = bearerHeader.split(\" \")[1];\r\n        req.token = bearerToken;\r\n        next();\r\n    } else {\r\n        res.sendStatus(403);\r\n    }\r\n}*/\r\n\r\nconst getodos = async (req, res) => {\r\n    try {\r\n        const {} = req.params;\r\n        const connection = await getConnection();\r\n        const data= await connection.query(\"SELECT * FROM cliente\");\r\n        res.json(data);\r\n    } catch (error) {\r\n        res.status(500);\r\n        res.send(error.message);\r\n    }\r\n}\r\n\r\n    \r\n// exportar metodos\r\nexport const methods = {\r\n    getAll,\r\n    add,\r\n    verificaruser,\r\n    getodos,\r\n    actualizardatos\r\n};\r\n\r\n\r\n\r\n\r\n"],"mappings":";;;;;;;;;AAAA;;;;+CACA,oJ;;;;;;AAAA,IAAIA,MAAM,GAAGC,OAAO,CAAC,UAAD,CAApB;;AACA,IAAIC,GAAG,GAAGD,OAAO,CAAC,cAAD,CAAjB,C,CAEA;;;AACA,IAAME,MAAM;EAAA,sEAAG,iBAAOC,GAAP,EAAYC,GAAZ;IAAA;IAAA;MAAA;QAAA;UAAA;YAAA;YAECC,UAFD,GAEgBF,GAAG,CAACG,MAFpB,CAECD,UAFD;YAAA;YAAA,OAGkB,IAAAE,uBAAA,GAHlB;;UAAA;YAGDC,UAHC;YAAA;YAAA,OAIWA,UAAU,CAACC,KAAX,CAAiB,6CAAjB,EAAgEJ,UAAhE,CAJX;;UAAA;YAIDK,IAJC;YAKPN,GAAG,CAACO,IAAJ,CAASD,IAAT;YALO;YAAA;;UAAA;YAAA;YAAA;YAOPN,GAAG,CAACQ,MAAJ,CAA